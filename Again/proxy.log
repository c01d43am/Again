name: CI

on: [push, pull_request]

jobs:
  build:
    runs-on: ubuntu-latest

    steps:
    - name: Checkout code
      uses: actions/checkout@v2

    - name: Set up Python
      uses: actions/setup-python@v2
      with:
        python-version: '3.x'

    - name: Install dependencies
      run: |
        python -m pip install --upgrade pip
        pip install -r requirements.txt

    - name: Run proxy and generate log
      run: |
        # Command to start the proxy and generate the log file
        ./start-proxy.sh > /home/runner/work/_temp/proxy.log
        echo "Proxy log generated"

    - name: Check if proxy.log exists
      run: |
        if [ ! -f /home/runner/work/_temp/proxy.log ]; then
          echo "proxy.log does not exist"
          exit 1
        else:
          echo "proxy.log exists"
        fi

    - name: Post-action step
      run: |
        # Your post-action step that requires proxy.log
        cat /home/runner/work/_temp/proxy.log